@using AspNetSampleMvcApp.Helpers
@model IEnumerable<AspNetSample.Core.DataTransferObjects.ArticleDto>

@{
    ViewData["Title"] = "View";
}

<h1>Articles</h1>

<a href="~/article/create">Create article</a>

@if (Model.Any())
{
    foreach (var item in Model)
    {
        @await Html.PartialAsync("ArticlePreview", item); // встраивание partial view
        //@{await Html.RenderPartialAsync("ArticlePreview");} - работает быстрее
    }
}

@Html.GeneratePagintator(new int[]{1,2,3,4,5,6})

