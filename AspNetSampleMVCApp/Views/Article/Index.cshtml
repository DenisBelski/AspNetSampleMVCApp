@using AspNetSampleMVCApp.Helpers
@model IEnumerable<AspNetSample.Core.DataTransferObjects.ArticleDto>

@{
    ViewData["Title"] = "View";
}

<h1>Articles</h1>


@if (Model.Any())
{
    foreach (var item in Model)
    {
        @await Html.PartialAsync("ArticlePreview", item);
        //Html.RenderPartialAsync - работает быстрее
    }
}


